{
  "name": "dd-models",
  "version": "0.14.0",
  "description": "Redefining database models",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "author": "Mgen (https://www.mgenware.com)",
  "homepage": "https://github.com/mgenware/dd-models",
  "bugs": "https://github.com/mgenware/dd-models/issues",
  "keywords": [
    "database",
    "orm",
    "sql generator"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mgenware/dd-models.git"
  },
  "license": "MIT",
  "scripts": {
    "test": "yarn run clean && yarn run compile && yarn run lint && yarn run t",
    "t": "jest --verbose=false",
    "build": "yarn test",
    "compile": "tsc -b tests",
    "dev": "yarn run clean && tsc -b tests -w",
    "prepublishOnly": "yarn run test",
    "lint": "tslint --project tsconfig.json --config tslint.json",
    "clean": "rimraf dist && rimraf dist_tests"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "jest": {
    "testRegex": "/dist_tests/.*(\\.|/)(test|spec)\\.jsx?$"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/lodash.camelcase": "^4.3.6",
    "@types/lodash.snakecase": "^4.1.6",
    "@types/node": "^12.0.4",
    "jest": "^24.8.0",
    "rimraf": "^2.6.3",
    "tslint": "^5.17.0",
    "typescript": "^3.5.1"
  },
  "dependencies": {
    "lodash.camelcase": "^4.3.0",
    "lodash.snakecase": "^4.1.1",
    "throw-if-arg-empty": "^2.0.1",
    "to-type-string": "^1.0.0"
  }
}
