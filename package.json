    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "eslint": "^6.5.1",
    "mocha": "^6.2.1",
    "rimraf": "^3.0.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "lodash.camelcase": "^4.3.0",
    "lodash.snakecase": "^4.1.1",
