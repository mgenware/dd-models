{
  "name": "mingru-models",
  "version": "0.25.0",
  "description": "Strongly typed database models in TypeScript",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "author": "Mgen (https://www.mgenware.com)",
  "homepage": "https://github.com/mgenware/mingru-models",
  "bugs": "https://github.com/mgenware/mingru-models/issues",
  "keywords": [
    "database",
    "orm",
    "sql generator"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mgenware/mingru-models.git"
  },
  "license": "MIT",
  "scripts": {
    "test": "yarn run clean && yarn run compile && yarn run lint && yarn run t",
